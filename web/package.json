{
  "name": "cfi-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --reporters=default --coverage=false",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:report": "playwright show-report",
    "e2e:prod": "npm run build && E2E_USE_START=true playwright test",
    "e2e:prod:chromium": "npm run build && E2E_USE_START=true playwright test --project=chromium"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.54.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.441.0",
    "next": "14.2.4",
    "next-themes": "^0.2.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "recharts": "^2.12.7",
    "swr": "^2.3.4",
    "tailwind-merge": "^2.3.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@playwright/test": "^1.47.2",
    "@types/jest": "^29.5.12",
    "@types/node": "24.2.0",
    "@types/react": "19.1.9",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.5",
    "undici": "^6.19.8",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.4.5"
  }
}
